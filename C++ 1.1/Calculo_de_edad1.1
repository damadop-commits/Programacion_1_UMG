Algoritmo Calculo_de_edad
	// Definicion de Variables
	Definir nombre Como Cadena
	Definir anioNacimiento, anioActual Como Entero //actualizacion de variable
	Definir edad Como Entero
	// Solicitar datos al usuario
	Escribir 'Ingrese Nombre Completo'
	Leer nombre
	Escribir 'Ingrese año de Nacimiento'
	Leer anioNacimiento
	// Calculo de Edad
	anioActual <- 2026
	edad <- anioActual-anioNacimiento
	// Resultado edad y persona que lo solicita
	Escribir nombre, ' ustede tiene: ', edad, ' años'
FinAlgoritmo
